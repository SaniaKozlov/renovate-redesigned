<script>
    if (screen.width >= 992) {
        var myHeader = $('#block-top-fixed');
        var layout = $('.layout');

        $(window).scroll(function(){
            var scroll = getScroll();
            if (scroll.top > 0) {
                myHeader.addClass('fixed');
                layout.addClass('padding-layout');
            } else {
                myHeader.removeClass('fixed');
                layout.removeClass('padding-layout');
            }
        });

        function getScroll () {
            var b = document.body;
            var e = document.documentElement;
            return {
                top: parseFloat(window.pageYOffset || b.scrollTop || e.scrollTop)
            };
        }
    }
</script>

